	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/Bart3kk/funky-friday-autoplay/main/script.lua"))()
  	end    
})

local Misc = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Msc = Misc:AddSection({
	Name = "Misc"
})

Misc:AddButton({
	Name = "Destroy GUI",
	Callback = function()
      		orionlib:Destroy()
  	end    
})

local Version = Misc:AddSection({
	Name = "Version"
})

tab:AddLabel("BTD Hub V1")


