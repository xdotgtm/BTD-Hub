local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

OrionLib:MakeNotification({
	Name = "BTD Hub V1 has Loaded",
	Content = "BTD Hub V1 has successfully loaded",
	Image = "rbxassetid://4483345998",
	Time = 5
})

local Window = OrionLib:MakeWindow({Name = "BTD Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "BTD Hub", IntroEnabled = true, IntroText = BTD Hub v1})

local TSB = Window:MakeTab({
	Name = "The Strongest Battlegrounds",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local ScriptHubs = TSB:AddSection({
	Name = "Script Hubs"
})

TSB:AddButton({
	Name = "cheate",
	Callback = function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/sandwichk/RobloxScripts/main/Scripts/BadWare/Hub/Load.lua", true))()
  	end    
})

TSB:AddButton({
	Name = "Legend Handles",
	Callback = function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/LOLking123456/Saitama111/main/battle121"))()
  	end    
})

local Movesets = TSB:AddSection({
	Name = "Movesets"
})

TSB:AddButton({
	Name = "Mixed Movesets",
	Callback = function()
            loadstring(game:HttpGet("https://pastebin.com/g7TkRqan"))()
  	end    
})

local FF = Window:MakeTab({
	Name = "Funky Friday",
